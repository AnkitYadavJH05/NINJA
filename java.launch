
import java .util.*;
class program63
{
    public static void main (String args[])
    {
        int n,sum=0,product=1;
        Scanner in = new Scanner (System.in);
        System.out.println("enter the number to check wether it is spy number or not");
        n=in.nextInt();
        for(int i=n;i>0;i=i/10)
        {
            int ld=i%10;
            product=product*ld;
            sum=sum+ld;
        }
        if(sum==product)
        {
         System.out.println(" the number is spy number:-"+n);   
        }
        else
        {
            System.out.println(" the number is not spy number:-"+n);   
    }

}
}
